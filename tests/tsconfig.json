{
  "compilerOptions": {
    "strictNullChecks": true,
    "lib": [ "ES2017", "webworker" ],
    "module": "esnext",
    "target": "esnext",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true,
    "types": [ "node" ],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "esModuleInterop": true
  }
}
